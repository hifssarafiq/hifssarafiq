import pandas as pd
#load the excel file
df=pd.read_excel('rowsfile.xlsx')
#display the first 10 rows of the file
print(df.head(10))
#display the last 10 rows of the dataframe
print("\nlast 10 rows (using tail()):")
print(df.tail(10))
#display the shapes of the dataframe (rows,columns)
print("\nshape of the dataframe(rows,columns):")
print(df.shape)
#display the basuc information about the dataframe
print("\ndataframe info(using info()):")
print(df.info())
#displaythe columns names
print("\ncolumn name(using columns):")
print(df.columns)
